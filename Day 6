dashboard

import React from 'react';
import '../Styles/DashBoard.css';

const Dashboard = () => {
  // Sample data for member statistics
  const memberStatistics = [
    { name: 'John Doe', age: 28, height: 175, gender: 'Male', trainingType: 'Weight Loss', membershipPack: '6 months' },
    { name: 'Jane Smith', age: 32, height: 165, gender: 'Female', trainingType: 'Strength Training', membershipPack: '1 year' },
    { name: 'Mike Johnson', age: 25, height: 180, gender: 'Male', trainingType: 'Cardio', membershipPack: '3 months' },
  ];

  return (
    <div className='dash'>
      <h1>Dashboard</h1>
      <div className="section">
        <h2>Recent Activities</h2>
        <table>
          <thead>
            <tr>
              <th>Activity</th>
              <th>Date</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>John Doe - Joined Gym</td>
              <td>2024-07-28</td>
              <td>Active</td>
            </tr>
            <tr>
              <td>Jane Smith - Completed Workout</td>
              <td>2024-07-28</td>
              <td>Completed</td>
            </tr>
            <tr>
              <td>Mike Johnson - Progress Updated</td>
              <td>2024-07-27</td>
              <td>Updated</td>
            </tr>
          </tbody>
        </table>
      </div>
      <div className="section">
        <h2>Member Statistics</h2>
        <table>
          <thead>
            <tr>
              <th>Name</th>
              <th>Age</th>
              <th>Height (cm)</th>
              <th>Gender</th>
              <th>Training Type</th>
              <th>Membership Pack</th>
            </tr>
          </thead>
          <tbody>
            {memberStatistics.map((member, index) => (
              <tr key={index}>
                <td>{member.name}</td>
                <td>{member.age}</td>
                <td>{member.height}</td>
                <td>{member.gender}</td>
                <td>{member.trainingType}</td>
                <td>{member.membershipPack}</td>
              </tr>
            ))}
          </tbody>
        </table>
      </div>
    </div>
  );
};

export default Dashboard;

//css

/* DashBoard.css */
.dash {
  font-family: Arial, sans-serif;
  padding: 20px;
  background-color: #f9f9f9;
}

.dash h1 {
  text-align: center;
  margin-bottom: 20px;
  color: #333;
}

.section {
  margin-bottom: 30px;
}

.section h2 {
  margin-bottom: 10px;
  color: #444;
}

table {
  width: 100%;
  border-collapse: collapse;
  margin-bottom: 20px;
}

table, th, td {
  border: 1px solid #ddd;
}

th, td {
  padding: 10px;
  text-align: left;
}

th {
  background-color: #f4f4f4;
  color: #333;
}

tr:nth-child(even) {
  background-color: #f9f9f9;
}

tr:hover {
  background-color: #f1f1f1;
}


